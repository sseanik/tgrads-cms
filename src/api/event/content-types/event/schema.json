{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Description": {
      "type": "text",
      "required": false
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Time": {
      "type": "string",
      "default": "5:30 PM"
    },
    "Location": {
      "type": "string",
      "required": true
    },
    "Suburb": {
      "type": "string"
    },
    "LocationURL": {
      "type": "string"
    },
    "Footnote": {
      "type": "richtext"
    },
    "Cost": {
      "type": "string",
      "default": "$"
    },
    "RSVPURL": {
      "type": "string",
      "default": "https://www.facebook.com/messages/t/6774431779249063"
    },
    "TGAEvent": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "State": {
      "type": "enumeration",
      "enum": [
        "NSW",
        "VIC",
        "QLD",
        "SA",
        "ACT",
        "WA",
        "TAS",
        "NT"
      ],
      "default": "NSW",
      "required": true
    }
  }
}
